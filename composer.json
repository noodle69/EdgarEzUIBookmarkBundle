{
  "name": "edgar/ez-uibookmark-bundle",
  "license": "GPL-2.0",
  "type": "ezplatform-bundle",
  "description": "eZ Platform bookmark content capability",
  "authors": [
    {
      "name": "Emmanuel DROUET"
    }
  ],
  "autoload": {
    "psr-4": {
      "Edgar\\EzUIBookmarkBundle\\": "src/bundle/",
      "Edgar\\EzUIBookmark\\": "src/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Edgar\\EzUIBookMarkBundle\\Tests\\": "src/bundle/Tests" }
  },
  "require": {
    "php": ">=7.1",
    "doctrine/orm": "^2.6",
    "ezsystems/ezpublish-kernel": "^7.1",
    "ezsystems/ezplatform-admin-ui": "^1.1",
    "edgar/ez-uiprofile-bundle": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.7.1",
    "phpunit/phpunit": "^6.4"
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
